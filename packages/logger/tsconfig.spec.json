{
  "extends": "@tsed/typescript/tsconfig.node.json",
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": "..",
    "declaration": false,
    "composite": false,
    "noEmit": true,
    "types": ["vite/client", "vitest/globals"]
  },
  "include": [
    "src/**/*.spec.ts",
    "vite.config.mts"
  ],
  "exclude": [
    "node_modules",
    "test",
    "lib",
    "benchmark",
    "coverage"
  ]
}
